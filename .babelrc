{
    "plugins": [
        [
            "module-resolver",
            {
                "cwd": "babelrc",
                "root": [
                    "./src"
                ],
                "extensions": [
                    ".js",
                    ".ios.js",
                    ".android.js"
                ],
                "alias": {
                    "_assets": "./src/assets",
                    "_components": "./src/components",
                    "_navigations": "./src/navigations",
                    "_screens": "./src/screens",
                    "_services": "./src/services",
                    "_utils": "./src/utils",
                    "_colours": "./src/colours",
                }
            }
        ]
    ]
}